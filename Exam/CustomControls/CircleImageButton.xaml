<UserControl x:Class="Exam.CustomControls.CircleImageButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Exam.CustomControls"
             xmlns:origin="clr-namespace:Exam"
             mc:Ignorable="d" 
             Width="70" Height="70" 
             x:Name="Control">
    <UserControl.Resources>
        <origin:TestConverter x:Key="testConverter"/>
    </UserControl.Resources>
    <Grid>
        <Image Source="{Binding ElementName=Control, Path=IconPath}" Width="64" Height="64"/>
        <Button Width="Auto" Height="Auto" Background="Transparent" BorderThickness="0" Click="Button_Click"/>
    </Grid>
</UserControl>
