{
  "Redirection": {
    "DownloadPath": "/download/index.html"
  },
  "JWT": {
    "Header": {
      "alg": "HS256",
      "typ": "JWT"
    },
    "ExpirationMins": 30,
    "RefreshTokenExpirationDays": 30,
    "Secret": "my_cool_secret"
  },
  "ConnectionStrings": {
    //"DefaultConnection": "FileName=/var/database/Staff.db",
    "DefaultConnection": "FileName=D:/Staff.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "http": {
        "Url": "http://*:5000"
      },
      "https": {
        "Url": "https://*:5001"
      }
    }
  }
}